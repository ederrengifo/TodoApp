<div class="wrapper">
  <div class="user_container">
  <div class="user_form">

  <h1 class="title_todo">Sign up</h1>
  <div class="description_todo">Register your account</div>

  <%= form_for @user do |form| %>
    <%= render partial: '/users/form', object: form %>
    <div class="user_actions">
      <div class="actions">
        <%= form.submit 'Register' %>
      </div>
    </div>
  <% end %>
  </div>

  <div class="user_footer">
    <p>Do you have an account?<%= link_to t(".sign_in"), sign_in_path %>
  </div>

  </div>
</div>
